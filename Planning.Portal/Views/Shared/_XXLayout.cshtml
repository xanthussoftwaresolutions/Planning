    <style type="text/css">
        option:checked, option:hover {
            color: white;
            background: #c8c8c8;
        }

        .form-control select:focus, select:active {
            border-color: #c8c8c8;
            outline: 0;
            -webkit-box-shadow: inset 0 1px 1px #c8c8c8, 0 0 8px #c8c8c8;
            box-shadow: inset 0 1px 1px #c8c8c8, 0 0 8px #c8c8c8;
        }

        #lstview select {
            color: white;
            background: #c8c8c8;
        }

        #item_Category,#item_BaseCategory {
            -webkit-appearance: menulist-button;
            cursor: pointer;
        }
        .btn:focus, .btn:active, .btn.active {
            background-color:#fff!important;
            border-color:none!important;
            color:#333!important;
        }

        .btn-default:hover, .btn-default:focus, .btn-default.focus, .btn-default:active, .btn-default.active, .open > .dropdown-toggle.btn-default{
            background-color:#fff!important;
            border-color:none!important;
        }

        #item_Category,#item_BaseCategory, #fromDate, #toDate,.btn {
            -webkit-align-items: center;
            -webkit-rtl-ordering: logical;
            -webkit-writing-mode: horizontal-tb;
            background-color: rgb(255, 255, 255);
            border-radius: 0px;
            border: 1px solid rgb(204, 204, 204);
            border-image-outset: 0px;
            border-image-repeat: stretch;
            border-image-slice: 100%;
            border-image-source: none;
            border-image-width: 1;
            box-sizing: border-box;
            color: rgb(85, 85, 85);
            display: inline-block;
            font-size: 14px;
            font-weight: normal;
            height: 30px;
            letter-spacing: normal;
            line-height: normal;
            margin: 0px 0px 10px 0px;
            padding: 4px 6px;
            text-align: start;
            text-indent: 0px;
            text-shadow: none;
            text-transform: none;
            vertical-align: middle;
            white-space: pre;
            word-spacing: 0px;
            width: 220px!important;
            text-align : left!important
        }
        .multiselect-container > li {
            width:218px;
        }
        .btn .caret {
            float:right;
            margin-top:7px;
        }

        input[type=button] {
            padding: 5px 15px;
            background: #186163;
            color: #ffffff;
            border: 0 none;
            cursor: pointer;
            -webkit-border-radius: 5px;
            border-radius: 5px;
        }

        .alert {
            margin-bottom: 9px !important;
            padding: 9px !important;
        }

        .headingPanel {
            background-color: #00a4b5 !important;
            border-color: #00a4b5 !important;
        }

        .btn-success, .btn-danger {
            background-color: #186163 !important;
            border-color: #186163 !important;
        }

        .lnkButton {
            display: block;
            width: 130px !important;
            height: 29px;
            background: #186163;
            padding: 5px 15px;
            text-align: center;
            -webkit-border-radius: 5px;
            border-radius: 5px;
            color: white;
            cursor: pointer;
            margin-left: 12px;
        }
            /* unvisited link */
            .lnkButton:link {
                color: white;
            }

            /* visited link */
            .lnkButton:visited {
                color: white;
            }

            /* mouse over link */
            .lnkButton:hover {
                color: white;
            }

        /* selected link */
        :active {
            color: white;
        }

        .clsProcess {
            display: none;
        }

        .clsExport {
            display: none;
        }

        .loading {
            /*border: 5px solid #67CFF5;*/
            border: none;
            display: none;
            background-color: White;
            z-index: 9999999999;
            height: 100%;
            width: 100%;
            overflow: hidden;
            position: absolute;
            opacity: 0.5;
            background-position: center;
            background-repeat: no-repeat;
        }

        #imgloader {
            position: absolute;
            top: 50%;
            left: 50%;
            z-index: 9999999999;
        }

        .overlay {
            position: absolute;
            width: 100%;
            height: 100%;
            background-color: rgba(0,0,0,0.4);
        }

        .modal1 {
            position: absolute !important;
            top: 40% !important;
            left: 25%;
            text-align: center;
            z-index: 9999;
        }

        .modal-dialog {
            width: 100% !important; /* New width for default modal */
        }

        .modal-sm {
            width: 350px; /* New width for small modal */
        }

        .modal-lg {
            width: 950px; /* New width for large modal */
        }

        .modal-header {
            height: 50px;
            padding-bottom: 3px;
            border: none;
        }

        .ui-iggrid .ui-iggrid-header, .ui-iggrid table {
            padding: 1px !important;
        }

        .ui-iggrid-multiheader-cell.ui-iggrid-headercell-featureenabled .ui-iggrid-headertext {
            text-indent: 1px !important;
        }

        .ui-iggrid-indicatorcontainer {
            min-width: 20px !important;
        }

        #loading_loading {
            left: 700px;
            top: 300px;
        }
        /*Import section CSS*/
        .container {
            margin-top: 0px;
        }

        .image-preview-input {
            position: relative;
            overflow: hidden;
            margin: 0px;
            color: #333;
            background-color: #fff;
            border-color: #ccc;
        }

            .image-preview-input input[type=file] {
                position: absolute;
                top: 0;
                right: 0;
                margin: 0;
                padding: 0;
                font-size: 20px;
                cursor: pointer;
                opacity: 0;
                filter: alpha(opacity=0);
            }

        .image-preview-input-title {
            margin-left: 2px;
        }

        input[type=submit] {
            padding: 5px 15px;
            background: #186163;
            color: #ffffff;
            border: 0 none;
            cursor: pointer;
            -webkit-border-radius: 5px;
            border-radius: 5px;
        }

        .modal {
            position: fixed;
            top: 0;
            left: 0;
            background-color: black;
            z-index: 99;
            opacity: 0.8;
            filter: alpha(opacity=80);
            -moz-opacity: 0.8;
            min-height: 100%;
            width: 100%;
        }
        .clsExport,clsProcess {
            display:none;
        }

        /*---------Import section CSS*/

        /*#customer {
            width: 100%;
        }*/
